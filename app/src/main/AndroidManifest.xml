<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.ptt.androidjetpack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChooseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".workmanager.WorkManagerActivity" />
        <activity android:name=".databinding.DataBinding" />
        <activity android:name=".pagingapi.PagingApiActivity" />
        <activity android:name=".navigation.NavigationActivity" />
        <activity android:name=".pagingroom.PagingRoomActivity"></activity>
    </application>

</manifest>